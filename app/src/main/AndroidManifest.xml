<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.miwok">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Miwok">

        <activity android:name=".colorsActivity2"
            android:label="Colors"

            android:parentActivityName=".MainActivity">
        </activity>

        <activity android:name=".familyActivity2"
            android:label="Family"

            android:parentActivityName=".MainActivity"/>

        <activity android:name=".phrasesActivity2"
            android:label="Phrases"

            android:parentActivityName=".MainActivity"/>

        <activity android:name=".numbersActivity2"
            android:label="Numbers"

            android:parentActivityName=".MainActivity"/>

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>